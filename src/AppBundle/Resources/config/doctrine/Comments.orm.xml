<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Comments" table="comments">
    <indexes>
      <index name="id_dogs" columns="id_dogs"/>
      <index name="id_services" columns="id_services"/>
    </indexes>
    <id name="idComments" type="integer" column="id_comments">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="klname" type="string" column="klname" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="phone" type="string" column="phone" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="question" type="text" column="question" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="idDogs" target-entity="Dogs" fetch="LAZY">
      <join-columns>
        <join-column name="id_dogs" referenced-column-name="id_dogs"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idServices" target-entity="Services" fetch="LAZY">
      <join-columns>
        <join-column name="id_services" referenced-column-name="id_services"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
