<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Nursery" table="nursery">
    <id name="idNursery" type="integer" column="id_nursery">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nameNur" type="string" column="name_nur" length="200" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="bdate" type="date" column="bdate" nullable="false"/>
    <field name="owner" type="string" column="owner" length="200" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="adress" type="string" column="adress" length="200" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="phone" type="string" column="phone" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cellphone" type="string" column="cellphone" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="photo" type="string" column="photo" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="site" type="string" column="site" length="200" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
