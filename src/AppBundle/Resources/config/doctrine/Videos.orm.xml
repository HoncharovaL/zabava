<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Videos" table="videos">
    <indexes>
      <index name="id_dogs" columns="id_dogs"/>
      <index name="id_news" columns="id_news"/>
    </indexes>
    <id name="idVideo" type="integer" column="id_video">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="video" type="string" column="video" length="300" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="idDogs" target-entity="Dogs" fetch="LAZY">
      <join-columns>
        <join-column name="id_dogs" referenced-column-name="id_dogs"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idNews" target-entity="News" fetch="LAZY">
      <join-columns>
        <join-column name="id_news" referenced-column-name="id_news"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
